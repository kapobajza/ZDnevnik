{
  "name": "sveltastic",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:translations": "npm-run-all --parallel vite typesafe-i18n",
    "vite": "vite dev",
    "build": "vite build",
    "start:prod": "vite build && node build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "sync": "svelte-kit sync",
    "lint": "eslint ./src",
    "format": "prettier --write .",
    "prepare": "svelte-kit sync",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typesafe-i18n": "typesafe-i18n",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@storybook/addon-essentials": "8.2.6",
    "@storybook/addon-interactions": "8.2.6",
    "@storybook/addon-links": "8.2.6",
    "@storybook/addon-svelte-csf": "5.0.0-next.1",
    "@storybook/blocks": "8.2.6",
    "@storybook/svelte": "8.2.6",
    "@storybook/sveltekit": "8.2.6",
    "@storybook/test": "8.2.6",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-node": "^5.2.2",
    "@sveltejs/enhanced-img": "0.3.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "3.1.1",
    "@tanstack/svelte-query-devtools": "5.51.21",
    "@testing-library/jest-dom": "6.4.8",
    "@testing-library/svelte": "5.2.1",
    "@testing-library/user-event": "14.5.2",
    "@types/cookie": "0.6.0",
    "@vitest/ui": "2.0.4",
    "autoprefixer": "^10.4.19",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-svelte": "^2.36.0",
    "eslint-plugin-tailwindcss": "3.17.4",
    "globals": "^15.0.0",
    "jsdom": "24.1.1",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "storybook": "8.2.6",
    "svelte": "5.0.0-next.210",
    "svelte-check": "^3.6.0",
    "sveltekit-superforms": "2.16.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0-alpha.20",
    "vite": "^5.0.3",
    "vitest": "2.0.4",
    "zod": "3.23.8"
  },
  "type": "module",
  "dependencies": {
    "@lottiefiles/svelte-lottie-player": "^0.3.1",
    "@tanstack/svelte-query": "5.51.21",
    "bits-ui": "^0.21.12",
    "clsx": "^2.1.1",
    "cookie": "0.6.0",
    "form-data-encoder": "^4.0.2",
    "formdata-node": "^6.0.3",
    "lucide-svelte": "^0.441.0",
    "tailwind-merge": "^2.4.0",
    "tailwind-variants": "^0.2.1",
